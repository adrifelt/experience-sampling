{
  "manifest_version": 2,

  "name": "Chrome User Experience Surveys",
  "description": "Want to help make Chrome better? This extension occasionally shows surveys to get your feedback.",
  "version": "3.0.3",

  "background": {
    "scripts": ["constants.js", "survey-submission.js", "url-handler.js", "background.js"],
    "persistent": false
  },

  "permissions": [
    "alarms",
    "experienceSamplingPrivate",
    "notifications",
    "storage",
    "tabs",
    "unlimitedStorage",
    "https://chrome-experience-sampling.appspot.com/_ah/api/*"
  ],

  "icons": {
    "16": "icons/cues_16.png",
    "48": "icons/cues_48.png",
    "128": "icons/cues_128.png"
  },

  "key": ""
}
